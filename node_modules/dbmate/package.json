{
  "name": "dbmate",
  "version": "1.0.3",
  "scripts": {
    "build": "tsc",
    "build:docs": "typedoc ./src/*.ts",
    "prepublish": "npm run build",
    "test": "jest",
    "prettier:check": "prettier --check 'src/**/*.ts' 'spec/**/*.ts'",
    "prettier:fix": "prettier --write 'src/**/*.ts' 'spec/**/*.ts'",
    "lint": "npm run prettier:check"
  },
  "repository": "gitlab:defunctzombie/node-dbmate",
  "main": "dist/index.js",
  "bin": {
    "dbmate": "dist/cli.js"
  },
  "files": [
    "yarn.lock",
    "package.json",
    "binaries",
    "dist",
    "LICENSE.md",
    "README.md"
  ],
  "devDependencies": {
    "@types/jest": "25.1.4",
    "@types/node": "13.9.0",
    "jest": "25.1.0",
    "prettier": "1.19.1",
    "ts-jest": "25.2.1",
    "typescript": "3.8.3"
  }
}
